<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="61.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17"
            fx:controller="com.piece.v1.BaseController">
    <HBox alignment="CENTER" prefHeight="103.0" prefWidth="1280.0" spacing="5.0" AnchorPane.rightAnchor="0.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <Button fx:id="homeB" alignment="CENTER" contentDisplay="BOTTOM" defaultButton="true" layoutX="10.0"
                minWidth="53.0" mnemonicParsing="false" onMouseClicked="#homeButton" prefHeight="100.0"
                prefWidth="152.0" style="-fx-background-color: #191414;" text="Home" textFill="#eeeeee">
            <font>
                <Font name="DIN Condensed Bold" size="15.0"/>
            </font>
        </Button>
        <Button fx:id="chatB" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onMouseClicked="#chatButton" prefHeight="100.0" prefWidth="152.0"
                style="-fx-background-color: #191414;" text="Chat" textFill="#eeeeee">
            <font>
                <Font name="DIN Condensed Bold" size="15.0"/>
            </font>
        </Button>
        <Button fx:id="aboutUsB" alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false"
                onMouseClicked="#aboutUsButton" prefHeight="100.0" prefWidth="152.0"
                style="-fx-background-color: #191414;" text="About" textFill="#eeeeee" wrapText="true">
            <font>
                <Font name="DIN Condensed Bold" size="15.0"/>
            </font>
        </Button>
        <Button fx:id="logoutB" alignment="CENTER" contentDisplay="BOTTOM" focusTraversable="false"
                layoutX="574.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#logoutButton"
                prefHeight="100.0" prefWidth="152.0" style="-fx-background-color: #191414;" text="Logout"
                textFill="#eeeeee" wrapText="true">
            <font>
                <Font name="DIN Condensed Bold" size="15.0"/>
            </font>
        </Button>
    </HBox>
</AnchorPane>
